<panel>

  <layout class="arrange_vertical_attachment" alignment="bottom,extend" offset="0"/>
  <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true"/>
  <container>
    <layout class="arrange_vertical_attachment" layout="-1" offset="0"/>
    <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true"/>

	
	  <container class="bordered_box" appearance="" inset="0" offset="0" gap="0">
	  <layout class="arrange_horizontal_attachment" layout="-8,2,-3" offset="0" gap="0"/>
	  <layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true"/>

	  	 
	<container>
		<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
		<!--球场图片-->
		<widget class="stadium_picture" id="STAp" image_alignment="centre_x, bottom"   keep_aspect_ratio="true" priority="1" default_height="200">
			<record id="object_property">
				<integer id="get_property" value="objt" />
				<integer id="set_property" value="objt" />
			</record>
		</widget>	
			

          </container>
	  
	  
      <container/>


      <container>
        <layout class="arrange_vertical_attachment" layout="-1,1,-2,1,-2,1,-2,1,-2,1" alignment="bottom,extend" offset="0"/>
        <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true"/>

		 <!--name-->
		<widget class="client_object_label" style="bold" id="nomV" size="12" font="title" colour="xingse" alignment="centre_y">
        <record id="object_property">
          <integer id="get_property" value="objt" />
        </record>
      </widget>
		<widget class="horizontal_divider" height="1"  />	
        <container>
          <layout class="arrange_vertical_attachment" layout="-1,-1" alignment="centre,extend" offset="0"/>
          <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true"/>


          <widget class="label" id="SctI" spec="text,small" colour="xingse" font="label" size="10" alignment="centre_x,centre" auto_size="vertical">
            <record id="object_property">
              <integer id="get_property" value="SctI" />
              <boolean id="name" value="true" />
            </record>
          </widget>
			<!--town-->
		  <widget class="city_label" spec="text" font="text"  id="nomL" size="12" colour="white"  alignment="centre_x,top" auto_size="vertical">
			<record id="object_property">
			  <integer id="get_property" value="SctI" />
			</record>
		  </widget>	  
        </container>


      	<widget class="horizontal_divider" height="1"  />		



          <container>
            <layout class="arrange_vertical_attachment" layout="-1,-1" alignment="bottom,extend" offset="0"/>
            <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true"/>

            <widget class="label" id="ScpL" spec="text,small" font="label" colour="xingse" size="10" alignment="centre_x,centre" auto_size="vertical">
              <record id="object_property">
                <integer id="get_property" value="Scpy" />
                <boolean id="name" value="true" />
              </record>
            </widget>
            <!--capacity-->
            <widget class="dashed_number_label" spec="text" font="text" id="ScpV" size="12" colour="white" alignment="centre_x,top" auto_size="vertical">
              <record id="object_property">
                <integer id="get_property" value="Scpy" />
              </record>
            </widget>
          </container>


      	<widget class="horizontal_divider" height="1"  />		


	   <container>
        <layout class="arrange_vertical_attachment" layout="-1,-1" alignment="bottom,extend" offset="0"/>
        <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true"/>
						
			        <widget class="label" id="SbdL" spec="text,small" font="label" alignment="centre_x,centre" size="10" auto_size="vertical" colour="xingse">
			          <record id="object_property">
			            <integer id="get_property" value="Sbdy" />
			            <boolean id="name" value="true" />
			          </record>
			        </widget>
			
			        <widget class="dashed_number_label" spec="text" font="text" id="SbdV" NumF="0" size="12" colour="white"  alignment="centre_x,top" auto_size="vertical">
			          <record id="object_property">
			            <integer id="get_property" value="Sbdy" />
			          </record>
			        </widget>
			      </container>	
				  
      	<widget class="horizontal_divider" height="1"  />		

				  
		<!--stadium condition-->
		 <container>
        <layout class="arrange_vertical_attachment" layout="-1,-1" alignment="bottom,extend" offset="0"/>
        <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true"/>

					<widget class="label" id="SdcL" spec="text,small"  font="label" colour="xingse" size="10" alignment="centre_x,centre"  auto_size="vertical">
						<record id="object_property">
							<integer id="get_property" value="Sdcy" />
							<boolean id="name" value="true" />
						</record>
					</widget>

					<widget class="stadium_condition_label" spec="text" font="text" id="SdcV" size="12" colour="white" alignment="centre_x,top"  auto_size="vertical">
						<record id="object_property">
							<integer id="get_property" value="Sdcy" />
						</record>
					</widget>
				</container>
				
      	<widget class="horizontal_divider" height="1"  />		


      </container>
	  	
				  
    </container>

	
</container>


</panel>