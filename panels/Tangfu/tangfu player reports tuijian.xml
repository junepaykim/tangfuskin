<panel>
	<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>


	<container class="player_reports_overview_summary_panel" file="none">
	<record id="object_property" get_property="objt" set_property="objt" />
		<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
		
		
		<list id="properties_to_forward">
			<record get_property="recommendation_widget_mode" set_property="recommendation_mode" target="sriW" />
			<record get_property="showing_in_list" set_property="showing_in_list" target="sriW" />
		</list>

		<container class="player_reports_overview_summary_panel" always_recolour_wage_cost_text="true" recommendation_widget_mode="0" mode="1" file="none">
		<layout class="vertical_arrange" vertical_layout="62,40,40,-1" vertical_gap="8"/>

		<record id="object_property">
					<list id="get_properties">
						<record>
							<integer id="get_property" value="Pspc" />
							<integer id="set_property" value="Pspc" />
						</record>
						<record>
							<integer id="get_property" value="Shlo" />
							<integer id="set_property" value="Shlo" />
						</record>
						<record>
							<integer id="get_property" value="Pers" />
							<integer id="set_property" value="objt" />
						</record>
						<record>
							<integer id="get_property" value="hpde" />
							<integer id="set_property" value="hidn" />
						</record>
					</list>
				</record>
	<container>
					<layout class="horizontal_arrange" horizontal_alignment="left,extend"/>

					<!-- AI Controlled -->
					<container id="SMbx" class="main_contrast_box_no_margin">
						<attachment_group class="horizontal_arrange_vertical_autosize_centre" horizontal_alignment="left,extend" horizontal_gap="4" horizontal_offset="6" vertical_offset="8" />

						<widget class="player_scout_recommendation_widget" id="sriW" width="40" height="40" show_knowledge_bar="false" navigation_focus_target="false">
							<record id="object_property" get_property="Psrk" set_property="objt"/>
						</widget>

						<widget class="text" id="recw" alignment="left,centre_y" multiline="true" style="bold" colour="background" auto_size="vertical" />

					</container>

					<!-- Human Controlled -->
					<container id="strS" class="plain_box" >
						<layout class="stick_to_sides_attachment" apply_to_children="true"/>

						<container class="person_chemistry_relationship_panel" file="none">
							<record id="object_property" get_property="objt" set_property="objt" />
							<attachment_group class="horizontal_arrange_vertical_autosize_centre" horizontal_alignment="right,extend" />

							<container class="vertical_label_value_pair" label_widget="Pptl" value_widget="Pptv">
								<attachment_group class="vertical_arrange" vertical_alignment="top" />

								<widget class="text" id="Pptl" auto_size="vertical" alignment="left,centre_y">
									<translation id="text" translation_id="492002" type="use" value="Agreed Playing Time[COMMENT: AREA: Player Happiness; SUB-AREA: Contract Promises; FR-BN: 14013; TONE: Formal; CHARLIMIT: None; REF-ID: n/a; NOTE: contract promise type]" />
								</widget>

								<widget class="client_object_property_panel" id="Pptv" property="Pcpt" auto_size="vertical">
									<record id="object_property">
										<integer id="get_property" value="objt" />
										<integer id="set_property" value="objt" />
									</record>
									<record id="widget_properties" alignment="left,centre_y" auto_size="vertical"/>
								</widget>

							</container>

							<widget class="button" id="chgb" auto_size="horizontal" click_event="EcPt" controller_button="x_button">
								<translation id="text" translation_id="295066" type="use" value="Change[COMMENT: button for changing a kit style]" />
							</widget>

						</container>

					</container>

				</container>	
				
        <container default_height="40">
                <layout class="arrange_horizontal_attachment" layout="-2,-2" offset="0" />
                <layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
                <container class="player_reports_overview_summary_panel" file="none">
                    <layout class="arrange_vertical_attachment" layout="20,20" offset="0" gap="0" />
                    <layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" inset="0" />
                    <record id="object_property" get_property="objt" set_property="objt" />
                    <widget class="text" font="title"  alignment="centre_x, centre" auto_size="vertical">
                        <translation id="text" translation_id="335037" type="use" value="Estimated Cost[COMMENT: scout report panel label]" />
                    </widget>
                    <widget class="text" id="cosw" size="large" alignment="centre_x, centre" style="semi_bold" auto_size="vertical" multiline="false" />
                </container>
                <container class="player_reports_overview_summary_panel" file="none">
                    <layout class="arrange_vertical_attachment" layout="20,20" offset="0" gap="0" />
                    <layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" inset="0" />
                    <record id="object_property" get_property="objt" set_property="objt" />
                    <widget class="label" font="title"  alignment="centre_x, centre" auto_size="vertical">
                        <translation id="text" translation_id="450789" type="use" value="Estimated Wage[COMMENT: FR 21116, scout report panel label]" />
                    </widget>
                    <widget class="text" id="ewaw" alignment="centre_x, centre" size="large" style="semi_bold" auto_size="vertical" multiline="false" />
                </container>
            </container>
            
			<container default_height="40">
                <layout class="arrange_horizontal_attachment" layout="-2,-2" offset="0" />
                <layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
                <container class="player_reports_overview_summary_panel" file="none">
                    <layout class="arrange_vertical_attachment" layout="20,20" offset="0" gap="0" />
                    <layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" inset="0" />
                    <record id="object_property" get_property="objt" set_property="objt" />
                    <widget class="label" font="title"  alignment="centre_x, centre" auto_size="vertical">
                        <translation id="text" translation_id="450790" type="use" value="Transfer Interest[COMMENT: FR 21116, scout report panel label]" />
                    </widget>
                    <widget class="text" id="triw" size="large" alignment="centre_x, centre" auto_size="vertical" multiline="false" />
                </container>
                <container class="player_reports_overview_summary_panel" file="none">
                    <layout class="arrange_vertical_attachment" layout="20,20" offset="0" gap="0" />
                    <layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" inset="0" />
                    <record id="object_property" get_property="objt" set_property="objt" />
                    <widget class="text" font="title"  alignment="centre_x, centre" auto_size="vertical">
                        <translation id="text" translation_id="450791" type="use" value="Loan Interest[COMMENT: FR 21116, scout report panel label]" />
                    </widget>
                    <widget class="text" id="loiw" size="large" alignment="centre_x, centre" auto_size="vertical" multiline="false" />
                </container>
            </container>
			
		<!--报告-->
			<container  minimum_height="200" priority="1" >
				<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true" inset="0" />

				<widget class="player_scout_reports_panel" id="psrp" file="Tangfu/tangfu player reports you1" late_loading="true" valid_for_unemployed_managers="false" valid_for_international_managers_other_team="false">

					<record id="object_property">
						<integer id="get_property" value="objt" />
						<integer id="set_property" value="objt" />
					</record>
				</widget>
			</container> 


	</container> 
	</container> 










</panel>